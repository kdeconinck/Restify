# Indicate that this is the 'ROOT' configuration file.
root = true

# End every file with a final newline.
[*]
insert_final_newline = true

# SECTION: Language Rules ('this.' and 'Me.' qualifiers).
[*.cs]
dotnet_style_qualification_for_field    = true
dotnet_style_qualification_for_property = true
dotnet_style_qualification_for_method   = true
dotnet_style_qualification_for_event    = true

# SECTION: Language Rules (Language keywords instead of framework type names for type references).
[*.cs]
dotnet_style_predefined_type_for_locals_parameters_members = true
dotnet_style_predefined_type_for_member_access             = true

# SECTION: Language Rules (Modifier preferences).
[*.cs]
csharp_preferred_modifier_order              = public, private, protected, internal, static, extern, new, virtual, abstract, sealed, override, readonly, unsafe, volatile, async
dotnet_style_require_accessibility_modifiers = always
dotnet_style_readonly_field                  = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_prefer_static_local_function          = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (Parentheses preferences).
[*.cs]
dotnet_style_parentheses_in_arithmetic_binary_operators = always_for_clarity
dotnet_style_parentheses_in_relational_binary_operators = always_for_clarity
dotnet_style_parentheses_in_other_binary_operators      = always_for_clarity
dotnet_style_parentheses_in_other_operators             = always_for_clarity # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (Expression-level preferences).
[*.cs]
dotnet_style_object_initializer                             = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_collection_initializer                         = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_auto_properties                         = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_explicit_tuple_names                           = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_inferred_tuple_names                    = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_inferred_anonymous_type_member_names    = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_conditional_expression_over_assignment  = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_conditional_expression_over_return      = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_compound_assignment                     = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_simplified_interpolation                = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_simplified_boolean_expressions          = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_inlined_variable_declaration                   = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_prefer_simple_default_expression                     = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_pattern_local_over_anonymous_function          = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_deconstructed_variable_declaration             = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_prefer_index_operator                          = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_prefer_range_operator                          = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_implicit_object_creation_when_type_is_apparent = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (Null-checking preferences).
[*.cs]
dotnet_style_coalesce_expression                                 = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_null_propagation                                    = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
dotnet_style_prefer_is_null_check_over_reference_equality_method = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_throw_expression                                    = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_conditional_delegate_call                           = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (File header preferences).
[*.cs]
file_header_template =  =====================================================================================================================\n= LICENSE:       Copyright (c) 2022 Kevin De Coninck\n=\n=                Permission is hereby granted, free of charge, to any person\n=                obtaining a copy of this software and associated documentation\n=                files (the "Software"), to deal in the Software without\n=                restriction, including without limitation the rights to use,\n=                copy, modify, merge, publish, distribute, sublicense, and/or sell\n=                copies of the Software, and to permit persons to whom the\n=                Software is furnished to do so, subject to the following\n=                conditions:\n=\n=                The above copyright notice and this permission notice shall be\n=                included in all copies or substantial portions of the Software.\n=\n=                THE SOFTWARE IS PROVIDED "AS IS", WITHOUT WARRANTY OF ANY KIND,\n=                EXPRESS OR IMPLIED, INCLUDING BUT NOT LIMITED TO THE WARRANTIES\n=                OF MERCHANTABILITY, FITNESS FOR A PARTICULAR PURPOSE AND\n=                NONINFRINGEMENT. IN NO EVENT SHALL THE AUTHORS OR COPYRIGHT\n=                HOLDERS BE LIABLE FOR ANY CLAIM, DAMAGES OR OTHER LIABILITY,\n=                WHETHER IN AN ACTION OF CONTRACT, TORT OR OTHERWISE, ARISING\n=                FROM, OUT OF OR IN CONNECTION WITH THE SOFTWARE OR THE USE OR\n=                OTHER DEALINGS IN THE SOFTWARE.\n=====================================================================================================================

# SECTION: Language Rules ('var' preferences).
[*.cs]
csharp_style_var_for_built_in_types    = false
csharp_style_var_when_type_is_apparent = true
csharp_style_var_elsewhere             = false

# SECTION: Language Rules (Expression-bodied members).
[*.cs]
csharp_style_expression_bodied_constructors    = false
csharp_style_expression_bodied_methods         = false
csharp_style_expression_bodied_operators       = when_on_single_line # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_expression_bodied_properties      = when_on_single_line
csharp_style_expression_bodied_indexers        = when_on_single_line # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_expression_bodied_accessors       = when_on_single_line
csharp_style_expression_bodied_lambdas         = when_on_single_line # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_expression_bodied_local_functions = false               # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (Pattern matching preferences).
[*.cs]
csharp_style_pattern_matching_over_as_with_null_check = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_pattern_matching_over_is_with_cast_check = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_prefer_switch_expression                 = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_prefer_pattern_matching                  = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.
csharp_style_prefer_not_pattern                       = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules (Code block preferences).
[*.cs]
csharp_prefer_braces                 = true
csharp_prefer_simple_using_statement = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# SECTION: Language Rules ('using' directive preferences).
[*.cs]
csharp_using_directive_placement = inside_namespace

# SECTION: Language Rules (Modifier preferences).
[*.cs]
csharp_prefer_static_local_function = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'.

# Section: Unnecessary code rules.
[*.cs]
csharp_style_unused_value_expression_statement_preference = discard_variable # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_style_unused_value_assignment_preference           = discard_variable # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
dotnet_code_quality_unused_parameters                     = all              # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
dotnet_remove_unnecessary_suppression_exclusions          = none             # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'

# Section: Formatting rules (Using directive options).
[*.cs]
dotnet_sort_system_directives_first                   = true
dotnet_separate_import_directive_groups               = true
dotnet_style_namespace_match_folder                   = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'

# Section: Formatting rules (New-line options).
[*.cs]
csharp_new_line_before_open_brace                     = all
csharp_new_line_before_else                           = true
csharp_new_line_before_catch                          = true
csharp_new_line_before_finally                        = true
csharp_new_line_before_members_in_object_initializers = true
csharp_new_line_before_members_in_anonymous_types     = true # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_new_line_between_query_expression_clauses      = true

# Section: Formatting rules (Indentation options).
[*.cs]
csharp_indent_case_contents            = true       # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_indent_switch_labels            = true
csharp_indent_labels                   = flush_left # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_indent_block_contents           = true       # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_indent_braces                   = false
csharp_indent_case_contents_when_block = false      # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'

# Section: Formatting rules (Spacing options).
[*.cs]
csharp_space_after_cast                                                  = false
csharp_space_after_keywords_in_control_flow_statements                   = true
csharp_space_between_parentheses                                         = false
csharp_space_before_colon_in_inheritance_clause                          = true
csharp_space_after_colon_in_inheritance_clause                           = true
csharp_space_around_binary_operators                                     = true
csharp_space_between_method_declaration_parameter_list_parentheses       = false
csharp_space_between_method_declaration_empty_parameter_list_parentheses = false
csharp_space_between_method_declaration_name_and_open_parenthesis        = false
csharp_space_between_method_call_parameter_list_parentheses              = false
csharp_space_between_method_call_empty_parameter_list_parentheses        = false
csharp_space_between_method_call_name_and_opening_parenthesis            = false
csharp_space_after_comma                                                 = true
csharp_space_before_comma                                                = false
csharp_space_after_dot                                                   = false
csharp_space_before_dot                                                  = false
csharp_space_after_semicolon_in_for_statement                            = true
csharp_space_before_semicolon_in_for_statement                           = false
csharp_space_around_declaration_statements                               = false # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_space_before_open_square_brackets                                 = false
csharp_space_between_empty_square_brackets                               = false
csharp_space_between_square_brackets                                     = false

# Section: Formatting rules (Wrap options).
[*.cs]
csharp_preserve_single_line_statements = false # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'
csharp_preserve_single_line_blocks     = false

# Section: Formatting rules (Namespace options).
[*.cs]
csharp_style_namespace_declarations = file_scoped # NOTE: Unsupported by 'JetBrains.ReSharper.GlobalTools'

# Section: Configuration for 'JetBrains.ReSharper.GlobalTools' - Blank Lines (General).
[*.cs]
csharp_blank_lines_around_region              = 1
csharp_blank_lines_inside_region              = 1
csharp_blank_lines_before_single_line_comment = 1

# Section: Configuration for 'JetBrains.ReSharper.GlobalTools' - Blank lines in declarations.
[*.cs]
csharp_keep_blank_lines_in_declarations                  = 0
csharp_remove_blank_lines_near_braces_in_declarations    = true 
