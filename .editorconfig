csharp_parentheses_non_obvious_operations       = none
csharp_parentheses_group_non_obvious_operations = none
csharp_parentheses_same_type_operations         = false

# Section: Configuration for 'JetBrains.ReSharper.GlobalTools' - Syntax style (Braces).
[*.cs]
