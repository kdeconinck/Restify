csharp_case_block_braces                               = next_line
csharp_initializer_braces                              = next_line
csharp_use_continuous_indent_inside_initializer_braces = true
csharp_other_braces                                    = next_line
csharp_allow_comment_after_lbrace                      = false
csharp_empty_block_style                               = together

# Section: Configuration for 'JetBrains.ReSharper.GlobalTools' - Syntax style ('var' usage in declarations﻿).
[*.cs]
csharp_for_built_in_types                                  = use_var_when_evident
csharp_for_other_types                                     = use_var_when_evident
csharp_use_roslyn_logic_for_evident_types                  = true
csharp_prefer_separate_deconstructed_variables_declaration = true
csharp_prefer_explicit_discard_declaration                 = false

# Section: Configuration for 'JetBrains.ReSharper.GlobalTools' - Syntax style (Instance members qualification﻿).
[*.cs]
csharp_instance_members_qualify_members     = all
